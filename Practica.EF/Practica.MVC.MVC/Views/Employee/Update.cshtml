@model Practica.MVC.MVC.Models.EmployeeView

@{
    ViewBag.Title = "Update de Employee";
}

<div class="navbar navbar-inverse">
    <div class="container">
        <div class="navbar-collapse collapse">
            <ul class="nav navbar-nav">
                <li>@Html.ActionLink("Select", "Index", "Employee")</li>
                <li>@Html.ActionLink("Insertar", "Insert", "Employee")</li>
                <li>@Html.ActionLink("Updatear", "Update", "Employee")</li>
                <li>@Html.ActionLink("Deletear", "Delete", "Employee")</li>
            </ul>
        </div>
    </div>
</div>

<h1>Updatear en Employee</h1>

@using (Html.BeginForm("Update", "Employee", FormMethod.Post))
{
    <label>Id: </label> @Html.TextBoxFor(e => e.Id) 
    <br />
    <label>Apellido: </label> @Html.TextBoxFor(e => e.LastName)
    <br />
    <label>Nombre: </label> @Html.TextBoxFor(e => e.FirstName)
    <br />
    <input type="submit" value="Enviar" class="btn btn-primary" />
}